{
  "name": "problem-h",
  "version": "0.0.0",
  "author": "Karolis Å arapnickis <pastas.k@gmail.com>",
  "license": "MIT",
  "private": false,
  "config": {
    "port": 3000
  },
  "scripts": {
    "develop": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --port ${PORT:=$npm_package_config_port}",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rimraf build",
    "test": "jest",
    "lint": "run-p lint:*",
    "lint:js": "eslint .",
    "lint:md": "markdownlint $(find . -name '*.md' -not -path '**/node_modules/*')",
    "format": "prettier --write '**/*.*'",
    "qa": "run-p lint test",
    "test-build": "yarn build && serve -s build",
    "precommit": "lint-staged",
    "prepush": "yarn qa"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "classnames": "^2.2.5",
    "font-awesome": "^4.7.0",
    "haversine": "^1.1.0",
    "idx": "^2.2.0",
    "offline-plugin": "^4.9.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-google-maps": "^9.4.5",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-sticky-el": "^1.0.16",
    "react-stickynode": "^1.4.1",
    "recompose": "^0.26.0",
    "redux": "^3.7.2",
    "redux-api-middleware": "^2.2.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-thunk": "^2.2.0",
    "seamless-immutable": "^7.1.3",
    "styled-components": "^3.1.6",
    "tachyons-sass": "^4.9.2",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-idx": "^2.2.0",
    "babel-preset-react-app": "^3.1.1",
    "compression-webpack-plugin": "^1.1.7",
    "copy-webpack-plugin": "^4.4.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "jest": "^22.3.0",
    "jest-fetch-mock": "^1.4.1",
    "lint-staged": "^6.1.1",
    "markdownlint-cli": "^0.7.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.10.2",
    "redux-mock-store": "^1.5.1",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "serve": "^6.4.9",
    "style-loader": "^0.20.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.{css,scss,md,json}": [
      "format",
      "git add"
    ]
  },
  "jest": {
    "testRegex": "src/.+\\.test\\.js$",
    "setupFiles": [
      "<rootDir>/src/__tests__/__setup__/fetch.js"
    ],
    "moduleNameMapper": {
      "^~/(.*)$": "<rootDir>/src/$1"
    }
  }
}
