{
  "name": "problem-h",
  "version": "0.0.0",
  "author": "Karolis Å arapnickis <pastas.k@gmail.com>",
  "license": "MIT",
  "private": false,
  "config": {
    "port": 3000
  },
  "scripts": {
    "develop": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.dev.js --port $npm_package_config_port",
    "build": "yarn clean && cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "clean": "rimraf build",
    "lint": "run-p lint:*",
    "lint:md": "markdownlint $(find . -name '*.md' -not -path '**/node_modules/*')",
    "test-build": "yarn build && serve -s build",
    "prepush": "yarn lint"
  },
  "dependencies": {
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-react-app": "^3.1.1",
    "compression-webpack-plugin": "^1.1.7",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "markdownlint-cli": "^0.7.1",
    "node-sass": "^4.7.2",
    "npm-run-all": "^4.1.2",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.6",
    "serve": "^6.4.9",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  }
}
